{
    "workbench.colorTheme": "statn-penta",
    "editor.fontFamily": "'JetBrains Mono', 'Droid Sans Mono', 'monospace', monospace",
    "editor.fontSize": 12,
    "editor.fontLigatures": true,
    "window.zoomLevel": 1.5,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<capslock>"
            ],
            "commands": [
                "whichkey.show"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<capslock>"
            ],
            "commands": [
                "whichkey.show"
            ]
        }
    ],
    "whichkey.bindingOverrides": [
        {
            "keys": "S.h",
            "name": "Show hover",
            "type": "command",
            "command": "editor.action.showDefinitionPreviewHover",
        },
        {
            "keys": "\t",
            "name": "Focus active editor",
            "type": "command",
            "command": "workbench.action.focusActiveEditorGroup"
        },
        {
            "keys": "e",
            "name": "+ Editor...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "a",
                    "name": "Focus active editor",
                    "type": "command",
                    "command": "workbench.action.focusActiveEditorGroup"
                },
                {
                    "key": "g",
                    "name": "+ Go to...",
                    "type": "bindings",
                    "bindings": [
                        {
                            "key": "h",
                            "name": "Previous editor",
                            "type": "command",
                            "command": "workbench.action.previousEditorInGroup"
                        },
                        {
                            "key": "l",
                            "name": "Next editor",
                            "type": "command",
                            "command": "workbench.action.nextEditorInGroup"
                        },
                        {
                            "key": "H",
                            "name": "Previous editor (all groups)",
                            "type": "command",
                            "command": "workbench.action.previousEditor",
                        },
                        {
                            "key": "L",
                            "name": "Next editor (all groups)",
                            "type": "command",
                            "command": "workbench.action.nextEditor",
                        },
                        {
                            "key": "[",
                            "name": "Previous group",
                            "type": "command",
                            "command": "workbench.action.focusPreviousGroup"
                        },
                        {
                            "key": "]",
                            "name": "Next group",
                            "type": "command",
                            "command": "workbench.action.focusNextGroup"
                        }
                    ]
                },
                {
                    "key": "m",
                    "name": "+ Move...",
                    "type": "bindings",
                    "bindings": [
                        {
                            "key": "H",
                            "name": "Move active editor group left",
                            "type": "command",
                            "command": "workbench.action.moveActiveEditorGroupLeft"
                        },
                        {
                            "key": "L",
                            "name": "Move active editor group right",
                            "type": "command",
                            "command": "workbench.action.moveActiveEditorGroupRight"
                        },
                        {
                            "key": "K",
                            "name": "Move active editor group up",
                            "type": "command",
                            "command": "workbench.action.moveActiveEditorGroupUp"
                        },
                        {
                            "key": "J",
                            "name": "Move active editor group down",
                            "type": "command",
                            "command": "workbench.action.moveActiveEditorGroupDown"
                        }
                    ]
                },
                {
                    "key": "s",
                    "name": "+ Split...",
                    "type": "bindings",
                    "bindings": [
                        {
                            "key": "h",
                            "name": "Split left",
                            "type": "command",
                            "command": "workbench.action.splitEditorLeft"
                        },
                        {
                            "key": "l",
                            "name": "Split right",
                            "type": "command",
                            "command": "workbench.action.splitEditorRight"
                        },
                        {
                            "key": "k",
                            "name": "Split up",
                            "type": "command",
                            "command": "workbench.action.splitEditorUp"
                        },
                        {
                            "key": "j",
                            "name": "Split down",
                            "type": "command",
                            "command": "workbench.action.splitEditorDown"
                        },
                        {
                            "key": "i",
                            "name": "Split inline",
                            "type": "command",
                            "command": "workbench.action.toggleSplitEditorInGroup"
                        }
                    ]
                },
                {
                    "key": "c",
                    "name": "+ Close ...",
                    "type": "bindings",
                    "bindings": [
                        {
                            "key": "c",
                            "name": "Close current editor",
                            "type": "command",
                            "command": "workbench.action.closeActiveEditor"
                        },
                        {
                            "key": "g",
                            "name": "Close current editor group",
                            "type": "command",
                            "command": "workbench.action.closeEditorsAndGroup"
                        },
                        {
                            "key": "o",
                            "name": "Close other editors in group",
                            "type": "command",
                            "command": "workbench.action.closeOtherEditors"
                        },
                        {
                            "key": "r",
                            "name": "Close editors to the right",
                            "type": "command",
                            "command": "workbench.action.closeEditorsToTheRight"   
                        },
                        {
                            "key": "s",
                            "name": "Close saved editors in group",
                            "type": "command",
                            "command": "workbench.action.closeUnmodifiedEditors"
                        },
                    ]
                },
                {
                    "key": "u",
                    "name": "Reopen last closed editor",
                    "type": "command",
                    "command": "workbench.action.reopenClosedEditor"
                },
            ]
        },
        {
            "keys": "p",
            "name": "+ Project...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "d",
                    "name": "New directory",
                    "type": "command",
                    "command": "explorer.newFolder"
                },
                {
                    "key": "f",
                    "name": "New file",
                    "type": "command",
                    "command": "explorer.newFile"   
                },
                {
                    "key": "e",
                    "name": "Explorer (Focus)",
                    "type": "command",
                    "command": "workbench.files.action.focusFilesExplorer"
                },
                {
                    "key": "c",
                    "name": "Collapse all folders",
                    "type": "command",
                    "command": "workbench.files.action.collapseExplorerFolders"
                }   
            ]
        },
        {
            "keys": "m",
            "name": "+ Marks...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "t",
                    "name": "Toggle bookmark",
                    "type": "command",
                    "command": "bookmarks.toggle"
                }, 
                {
                    "key": "n",
                    "name": "Next bookmark",
                    "type": "command",
                    "command": "bookmarks.jumpToNext"
                },
                {
                    "key": "p",
                    "name": "Previous bookmark",
                    "type": "command",
                    "command": "bookmarks.jumpToPrevious"
                },
                {
                    "key": "l",
                    "name": "List bookmarks",
                    "type": "command",
                    "command": "bookmarks.list"
                },
                {
                    "key": "a",
                    "name": "List bookmarks in all files",
                    "type": "command",
                    "command": "bookmarks.listFromAllFiles"
                },
                {
                    "key": "c",
                    "name": "+ Clear...",
                    "type": "bindings",
                    "bindings": [
                        {
                            "key": "f",
                            "name": "Clear bookmarks in file",
                            "type": "command",
                            "command": "bookmarks.clear"
                        },
                        {
                            "key": "a",
                            "name": "Clear bookmarks in all files",
                            "type": "command",
                            "command": "bookmarks.clearFromAllFiles"
                        }
                    ]
                }
            ]
        },
        {
            "keys": "w",
            "name": "+ Window ...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "t",
                    "name": "Terminal",
                    "type": "command",
                    "command": "workbench.action.terminal.toggleTerminal"
                },
                {
                    "key": "p",
                    "name": "Problems",
                    "type": "command",
                    "command": "workbench.actions.view.problems"
                },
                {
                    "key": "o",
                    "name": "Output",
                    "type": "command",
                    "command": "workbench.action.output.toggleOutput"
                },
                {
                    "key": "d",
                    "name": "Debug console",
                    "type": "command",
                    "command": "workbench.debug.action.toggleRepl"
                },
                {
                    "key": "n",
                    "name": "+ Notifications...",
                    "type": "bindings",
                    "bindings": [
                        {
                            "key": "s",
                            "name": "Show/hide notifications",
                            "type": "command",
                            "command": "notifications.toggleList"
                        },
                        {
                            "key": "c",
                            "name": "Clear notifications",
                            "type": "command",
                            "command": "notifications.clearAll"
                        },
                        {
                            "key": "q",
                            "name": "Toggle do not disturb mode (no notifications)",
                            "type": "command",
                            "command": "notifications.toggleDoNotDisturbMode"
                        }
                    ]
                }
            ]
        },
        {
            "keys": "z",
            "name": "test",
            "type": "command",
            "command": "workbench.action.toggleSplitEditorInGroup"
        }
    ],
    "workbench.settings.applyToAllProfiles": [

    ],
    "explorer.confirmDragAndDrop": false,
    "javascript.updateImportsOnFileMove.enabled": "never",
}